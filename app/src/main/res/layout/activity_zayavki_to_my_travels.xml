<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:background="@drawable/background"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.poputchic.android.activities.ZayavkiToMyTravels">
    <ImageView
        android:id="@+id/toolbar_my_travels"
        android:background="@color/black_100"
        android:layout_width="match_parent"
        android:layout_height="70dp" />
    <TextView
        android:id="@+id/activity_zayavki_to_my_travels_logo"
        android:fontFamily="@font/comforta_light"
        android:layout_marginTop="20dp"
        android:layout_centerHorizontal="true"
        android:textSize="22sp"
        android:textColor="@color/yallou"
        android:text="@string/app_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <!--<ImageView
        android:background="@color/yallou"
        android:layout_below="@id/toolbar_my_travels"
        android:layout_width="match_parent"
        android:layout_height="240dp" />-->
    <TextView
        android:fontFamily="@font/comforta_light"
        android:id="@+id/logo_my_travels"
        android:layout_marginTop="40dp"
        android:layout_below="@id/toolbar_my_travels"
        android:layout_centerHorizontal="true"
        android:textSize="20sp"
        android:textColor="@color/black_100"
        android:text="@string/zayavki"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <ListView
        android:scrollIndicators="none"
        android:divider="@color/gray_10"
        android:dividerHeight="0dp"
        android:background="@color/gray_10"
        android:layout_marginTop="50dp"
        android:layout_centerHorizontal="true"
        android:id="@+id/list_my_travels"
        android:layout_below="@id/logo_my_travels"
        android:layout_width="300dp"
        android:layout_height="wrap_content">
    </ListView>

</RelativeLayout>
